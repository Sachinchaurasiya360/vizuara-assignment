  "description": "Production-ready ML Pipeline Builder Backend with Node.js, TypeScript, Danfo.js, and Scikit.js",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "machine-learning",
    "pipeline",
    "scikit",
    "danfo",
    "typescript",
    "nodejs"
